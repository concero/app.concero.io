		"posthog-js": "^1.157.2",
		"rango-sdk": "^0.1.40",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-i18next": "^13.3.1",
		"react-router-dom": "^6.16.0",
		"react-tooltip": "^5.28.0",
