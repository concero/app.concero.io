.container {
	flex-direction: column;
	width: 100%;
	gap: var(--sp-md);

	.link {
		width: 100%;
		border: unset;
		padding: unset;

		.listButton {
			justify-content: flex-start;
			width: 100%;
		}
	}

	.separator {
		width: 100%;
		height: 1px;
		background-color: var(--color-grey-600);
		border-radius: var(--st-br-md);
	}
}

.active {
	background-color: var(--color-primary-600);
}
