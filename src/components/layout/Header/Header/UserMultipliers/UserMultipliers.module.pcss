.userMultipliers {
	display: flex;
	justify-content: space-between;
	min-width: 261px;
	background-color: var(--color-grey-700);
	gap: 12px;
}

.multiplierHeading {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}

.heading {
	color: var(--color-grey-100) !important;
	font-size: 16px !important;
	font-weight: 500;
}

.multiplierComponents {
	display: flex;
	flex-direction: column;
	justify-content: space-between;
	gap: 8px;
}

.item {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	line-height: 18px !important;
	align-items: center;

	span {
		color: var(--color-grey-300);
	}
}

.item > span {
	color: var(--color-grey-300) !important;
	font-size: 14px;
}

.item > .multiplier {
	color: var(--color-grey-100) !important;
}
