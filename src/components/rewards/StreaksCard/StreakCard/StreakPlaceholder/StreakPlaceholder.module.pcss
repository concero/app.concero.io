.container {
	max-width: 292px;
	width: 100%;
	gap: var(--sp-lg);
}

.tooltipWrap {
	max-width: 262px;
	width: 100%;
}

.separator {
	width: 1px;
	height: 100%;
	margin: 0 var(--sp-sm);
	background-color: var(--color-gray-200);
}

.date {
	font-size: 12px;
	color: var(--color-gray-500);
	font-weight: 400;
}

@media (max-width: 768px) {
	.container {
		max-width: 100%;
	}
}
