	flex-direction: row;
	align-items: center;
	padding-bottom: var(--sp-md);
	transition: 200ms ease-in-out;
}

.spaceBetweenContainer {
	justify-content: space-between;
}

.settingsContainer {
	flex-direction: row;
	align-items: center;
	gap: var(--sp-md);
}

.toggleTestnetContainer {
	flex-direction: row;
	align-items: center;
	gap: var(--sp-md);
}
