.container {
	flex: 1;
	height: 80px;
	justify-content: space-between;
	border: 1px solid var(--color-primary-650);
	background-color: var(--color-primary-700);
}

.titleContainer {
	h5 {
		color: var(--color-primary-400);
	}
}

.infoContainer {
	width: 100%;
	flex-direction: row;
	justify-content: space-between;
	align-items: flex-end;

	h3,
	h4 {
		line-height: 100%;
	}

	h4 {
		color: var(--color-primary-400);
	}
}

.tagText {
	color: var(--color-primary-350);
}
