.social_wrap {
	display: flex;
	flex-direction: row;
	justify-content: space-between;
	align-items: center;
	width: 100%;
	height: 64px;
	padding: var(--space-m);
	border-radius: var(--space-m);
	background-color: var(--color-gray-50);
	.social_header {
		display: flex;
		flex-direction: row;
		align-items: center;
		gap: var(--space-xs);
		font: var(--font-heading-medium);
		color: var(--color-gray-700);
	}
}
