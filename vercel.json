{
  "rewrites": [
    {
      "source": "/posthog/:path(.*)",
      "destination": "https://eu.posthog.com/:path*"
    },
    {
      "source": "/([^.\n]*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/signup/:path*",
      "destination": "https://send.concero.io/:path*"
    }
  ]
}
